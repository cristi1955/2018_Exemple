(function(){define(["jquery"],function(r){return(function(){function t(r){var t;this.constructor._show_hi_res()&&(t=r.attr("data-hi-res-no-resize"),this.constructor.set_src(r,r.attr("data-hi-res"),t),this.constructor.set_bg(r,r.attr("data-hi-res-background"),t))}return t.set_src=function(r,t,i){if(this._show_hi_res())return t&&t!==r.attr("src")?this._replace(r,t,!i,this._replace_img):void 0},t.set_bg=function(r,t,i){if(this._show_hi_res())return this._replace(r,t,!i,this._replace_background)},t._show_hi_res=function(){return"devicePixelRatio"in window&&window.devicePixelRatio>1},t._replace=function(t,i,e,n){var c;if(i)return c=new Image,c.src=i,r(c).on("load",function(){return n(t,i,e,c.width,c.height)})},t._replace_img=function(r,t,i,e,n){if(r.attr("src",t),i)return r.width(e/2),r.height(n/2)},t._replace_background=function(r,t,i,e,n){var c;if(c="url('%(src)s')".format({src:t}),r.css("backgroundImage",c),i)return r.css("backgroundSize",e/2+"px "+n/2+"px")},t})()})}).call(this);
